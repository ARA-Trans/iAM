{
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [ true, "spaces", 2 ],
    "no-consecutive-blank-lines": false,
    "curly": true,
    "new-parens": true,
    "semicolon": true,
    "import-spacing": true
  },
  "defaultSeverity": "warning",
  "linterOptions": {
    "exclude": [ "node_modules/**" ]
  }
}
